{
  "name": "freezr",
  "version": "0.0.13",
  "description": "Immutable data with native interface (and some sugar)",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "bugs": {
    "url": "https://github.com/carlesba/freezr/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carlesba/freezr"
  },
  "scripts": {
    "test": "npm run linter; npm run testing; npm run clean",
    "testing": "tape -r babel-register src/**/*.spec.js -o | faucet",
    "dev": "tape-watch -r babel-register src/**/*.spec.js -o | faucet",
    "dev:raw": "tape-watch -r babel-register src/**/*.spec.js",
    "clean": "rm -rf lib",
    "linter": "standard src | snazzy",
    "build": "./node_modules/.bin/babel src --out-dir lib",
    "prepublish": "npm run test && npm run clean && npm run build",
    "postpublish": "npm run clean",
    "try": "webpack-dev-server --config ./try/webpack.config.js"
  },
  "keywords": [
    "immutable",
    "javascript",
    "model"
  ],
  "author": "Carles Ballester <carlesba.js@gmail.com> @carlesba",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.5.2",
    "babel-register": "^6.23.0",
    "expect": "^1.20.1",
    "faucet": "^0.0.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "tape": "^4.7.0",
    "tape-watch": "^2.3.0",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "xit"
    ]
  },
  "dependencies": {}
}
