{
  "name": "freezr",
  "version": "0.0.2",
  "description": "Immutable data with native interface (and some sugar)",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register --recursive",
    "dev:watch": "mocha --compilers js:babel-register --recursive -w",
    "dev:reporter": "mocha --compilers js:babel-register --recursive -w --reporter mocha-osx-reporter",
    "start": "npm run dev:watch",
    "clean": "rimraf dist",
    "build": "./node_modules/.bin/babel src --out-dir lib",
    "prepublish": "npm run test && npm run clean && npm run build"
  },
  "keywords": [
    "immutable",
    "javascript",
    "model"
  ],
  "author": "Carles Ballester <carlesba.js@gmail.com> @carlesba",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.6.5",
    "expect": "^1.14.0",
    "mocha": "^2.4.5",
    "mocha-osx-reporter": "^0.1.2",
    "rimraf": "^2.5.2",
    "standard": "^6.0.7"
  },
  "standard": {
    "globals": [
      "describe",
      "it"
    ]
  }
}
